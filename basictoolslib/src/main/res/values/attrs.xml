<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--MultiStateLayout-->
    <declare-styleable name="MultiStateLayout">
        <attr name="loading_view" format="reference" />
        <attr name="error_view" format="reference" />
        <attr name="empty_view" format="reference" />
        <attr name="view_state" format="enum">
            <enum name="content" value="0" />
            <enum name="loading" value="1" />
            <enum name="error" value="2" />
            <enum name="empty" value="3" />
        </attr>
    </declare-styleable>

</resources>